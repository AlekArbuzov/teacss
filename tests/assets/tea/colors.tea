#yelow {
  #short {
    color: #fea;
  }
  #long {
    color: #ffeeaa;
  }
  #rgba {
    color: rgba(255, 238, 170, 0.1);
  }
  #argb {
    color: @{argb(0.1, 255, 238, 170)} ;
  }
}

#blue {
  #short {
    color: #00f;
  }
  #long {
    color: #0000ff;
  }
  #rgba {
    color: rgba(0, 0, 255, 0.1);
  }
  #argb {
    color: @{argb(0.1, 0, 0, 255)};
  }
}

#alpha #hsla {
    color: hsla(11, 20%, 20%, 0.6);
}

#overflow {
  .a { color: @{sub_colors(color('#111111'), color('#444444'))}; } // #000000
  .b { color: @{add_colors(color('#eee'), color('#fff'))}; } // #ffffff
  .d { color: @{add_colors(color('#00ee00'), color('#009900'))}; } // #00ff00
}

#grey {
  color: rgb(200, 200, 200);
}

#808080 {
  color: hsl(50, 0%, 50%);
}

#00ff00 {
  color: hsl(120, 100%, 50%);
}

.lightenblue {
    color: @{lighten('blue', 10)};
}

.darkenblue {
    color: @{darken('blue', 10)};
}
