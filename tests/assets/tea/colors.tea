#yellow {
  #short {
    color: #fea;
  }
  #long {
    color: #ffeeaa;
  }
  #rgba {
    color: rgba(255, 238, 170, 0.1);
  }
@{
    col = new teacss.Color(255, 238, 170, 1);
    val = col.toHSL();
}
  #argb {
    color: @val;
  }
}

#blue {
  #short {
    color: #00f;
  }
  #long {
    color: #0000ff;
  }
  #rgba {
    color: rgba(0, 0, 255, 0.1);
  }
  #argb {
    color: argb(rgba(0, 0, 255, 0.1));
  }
}

#alpha #hsla {
    color: hsla(11, 20%, 20%, 0.6);
}

@ v1 = 0xdddddd;
@ v2 = 0x111111;

@{
    a = v2 - v1;
    if (a < 0) { a = "#000000"; }
    b = "#" + (v1 + v2).toString(16);
    c = v1 * v2;
    if (c > 0xffffff) {c = "#ffffff"; }
}

#overflow {
  .a { color: @a } // #000000
  .b { color: @b } // #eeeeee
  .c { color: @c } // #ffffff
}

#grey {
  color: rgb(200, 200, 200);
}

#808080 {
  color: hsl(50, 0%, 50%);
}

#00ff00 {
  color: hsl(120, 100%, 50%);
}

.lightenblue {
    color: lighten(blue, 10%);
}

.darkenblue {
    color: darken(blue, 10%);
}

.unknowncolors {
    color: blue2;
    border: 2px solid superred;
}
