#functions {
  @{var teavar = 10}
  color: @{color("beige")}; //#f5f5dc
  width: @{++teavar};
  border-width: @{2 + 3};
}

#built-in {
  @ var r = 32
  lighten: @{lighten("#ff0000", 40)};
  darken: @{darken("#ff0000", 40)};
  saturate: @{saturate("#29332f", 20)};
  desaturate: @{desaturate("#203c31", 20)};
  greyscale: @{greyscale("#203c31")};
  spin-p: @{spin(hsl(340, 50, 50), 40)};
  spin-n: @{spin(hsl(30, 50, 50), -40)};

  hue: @{hue(hsl(98, 12, 95))};
  saturation: @{saturation(hsl(98, 12, 95))};
  lightness: @{lightness(hsl(98, 12, 95))};
  rounded: @{Math.round(r/3)};
  roundedpx: @{Math.round(10 / 3)}px;
  color: @{color("#ff0011")};

  .is-a {
    color: @{color("#ddd") instanceof teacss.Color};
    color: @{color('red') instanceof teacss.Color};
    color: @{rgb(0, 0, 0) instanceof teacss.Color};
    number: @{(parseFloat("32") != NaN)};
  }
}
